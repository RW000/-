<view>
	<form bindsubmit='login'>
		<view class="first">
			<input id="username" placeholder="请输入用户名" placeholder-class="plas" class="inputs" type="text" name="login_name" /></view>
      <view class="second">
        <input id="password" placeholder="请输入密码" placeholder-class="plas" class="inputs" type="password" name="login_pass" />
      </view>

<button class="loginBtn" type="primary" form-type="submit">登录</button>
<view class="cha" bindtap="enroll"><text class="no">没有账号，点我注册</text></view>
	</form>
</view>
